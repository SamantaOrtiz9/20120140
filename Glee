% regla personaje de
personaje_actor(X, Y) :- personajede(X, Y).

% apellido del actor
apellido_actor(X,Y):- apellidoactor(X,Y).

% apellido del personaje
apellido_personaje(X,Y):- apellidopersonaje(X,Y).

$ regla estado
estado_actual(X,Y):- estado(X,Y).

$ regla causa de muerte
muerte(X,Y):- causa(X,Y).

% regla para saber quien es madre de quien 
es_madre(X,Y):- madrede(X,Y).

% regla para saber quien es padre de quien 
es_padre(X,Y):- padrede(X,Y).

% fecha de la serie //
fecha_serie(X,Y):- fechade(X,Y).

% inicio en la serie
inicio_enserie(X,Y):- inicio(X,Y).

% fin en la serie
fin_enserie(X,Y):- fin(X,Y).

% quien beso a quien
beso_a(X,Y):- besoa(X,Y).

% shippeos importantes 
shippeo(X,Y):- ship(X,Y);

% primera cancion en la serie
cancion(X,Y,z):- primeracancionde(X,Y).

% capitulos por temporada 
capitulos_temporada(X,Y):- capitulosxtemporada(X,Y).

% mis favoritos
preferencia_por(X,Y):- preferencia(X,Y).
